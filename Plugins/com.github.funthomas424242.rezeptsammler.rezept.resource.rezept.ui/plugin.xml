			</description>
			<contextualLaunch>
				<enablement>
					<with variable="selection">
						<count value="1">
						</count>
						<iterate>
							<test property="com.github.funthomas424242.rezeptsammler.rezept.isLaunchable">
							</test>
						</iterate>
					</with>
				</enablement>
			</contextualLaunch>
